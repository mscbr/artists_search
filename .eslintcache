(250px, 1fr));\n    grid-template-rows: repeat(auto-fill, auto);\n    grid-gap: 16px;\n  }\n`;\n\nconst ArtistsSearch = () => {\n  const history = useHistory();\n  const { searchVal } = useParams<{ searchVal: string }>();\n  const favourites = useSelector(\n    (state: AppState) => state.favReducer.favourites\n  )?.map((item: { name: string; mbid: string }) => item.mbid);\n  const [searchValue, setSearchValue] = useState<string>(searchVal || '');\n  const [\n    getArtists,\n    { loading, data, fetchMore, networkStatus, error }\n  ] = useLazyQuery<IArtistsSearch>(GET_ARTISTS, {\n    notifyOnNetworkStatusChange: true\n  });\n\n  const fetch = useRef(\n    _.debounce((val: string) => {\n      getArtists({ variables: { artist: val, after: null } });\n      history.push(`/artists/${val}`);\n    }, 500)\n  ).current;\n\n  const handleChange = ({\n    target: { value }\n  }: React.ChangeEvent<HTMLInputElement>) => {\n    if (value) fetch(value);\n    setSearchValue(value);\n  };\n  const handleEnter = () => {\n    const cursor = data?.search?.artists?.pageInfo?.endCursor;\n    if (\n      fetchMore === undefined ||\n      loading ||\n      !data?.search?.artists?.pageInfo.endCursor\n    )\n      return;\n    fetchMore({\n      variables: { after: cursor },\n      updateQuery: (prev, { fetchMoreResult }) => {\n        if (!fetchMoreResult) return prev;\n        fetchMoreResult.search.artists.nodes = [\n          ...prev.search.artists.nodes,\n          ...fetchMoreResult.search.artists.nodes\n        ];\n        return fetchMoreResult;\n      }\n    });\n  };\n  const artists = searchValue && data?.search?.artists?.nodes;\n\n  useEffect(() => {\n    if (searchValue && !data)\n      getArtists({ variables: { artist: searchValue, after: null } });\n  }, []);\n\n  return (\n    <Layout>\n      <StyledContainer>\n        <StyledTitle>ARTIST SEARCH</StyledTitle>\n        <StyledSearchContainer>\n          <Search\n            id=\"artists-search\"\n            label=\"Search for your favourite aritsts...\"\n            value={searchValue}\n            onChange={handleChange}\n            icon={loading && !!searchValue && <Spinner />}\n            error={!!error}\n            helperText={(error && 'Failed to load data') || ''}\n          />\n        </StyledSearchContainer>\n        <StyledList>\n          {artists &&\n            artists.map((artist, i) => {\n              const isFav = favourites && favourites.includes(artist.mbid);\n              return (\n                <React.Fragment key={`${artist.mbid}${i}`}>\n                  <Artist artist={artist} isFav={isFav} />\n                  {i === artists.length - 1 && !!fetchMore && (\n                    <Waypoint onEnter={handleEnter} />\n                  )}\n                </React.Fragment>\n              );\n            })}\n          {networkStatus === 3 && <Spinner />}\n        </StyledList>\n      </StyledContainer>\n    </Layout>\n  );\n};\n\nexport default ArtistsSearch;\n","/Users/ckln/Documents/Projects/artists_search/src/api/handlers/artists.ts",[],"/Users/ckln/Documents/Projects/artists_search/src/config.ts",[],"/Users/ckln/Documents/Projects/artists_search/src/components/search/index.tsx",[],"/Users/ckln/Documents/Projects/artists_search/src/components/spinner/index.tsx",[],"/Users/ckln/Documents/Projects/artists_search/src/shared/hooks/useWindowWidth.ts",[],"/Users/ckln/Documents/Projects/artists_search/src/components/typography/detail.tsx",[],"/Users/ckln/Documents/Projects/artists_search/src/components/typography/title.tsx",[],"/Users/ckln/Documents/Projects/artists_search/src/components/typography/subtitle.tsx",[],"/Users/ckln/Documents/Projects/artists_search/src/components/typography/description.tsx",[],"/Users/ckln/Documents/Projects/artists_search/src/components/surface/index.tsx",[],"/Users/ckln/Documents/Projects/artists_search/src/components/button/index.tsx",[],"/Users/ckln/Documents/Projects/artists_search/src/components/artist/index.tsx",[],"/Users/ckln